{
  "name": "nginx-web",
  "description": "Public web tier using private registry credentials",
  "spec": {
    "source": {
      "kind": "containerImage",
      "containerImage": {
        "image": "registry.example.com/nginx",
        "tag": "1.23.0",
        "ports": [80],
        "credentialsSecretRef": {
          "name": "registry-creds",
          "namespace": "tn-acme-app-shop"
        }
      }
    }
  }
}
