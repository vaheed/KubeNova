# -----------------------------------------------------------------------------
# KubeNova environment defaults (.env.example)
# -----------------------------------------------------------------------------
KUBENOVA_DB_URL=postgres://kubenova:kubenova@localhost:5432/kubenova?sslmode=disable
KUBENOVA_REQUIRE_AUTH=true
# 32-byte base64 keys (generate: `openssl rand -base64 32`)
KUBENOVA_JWT_SIGNING_KEY=replace-with-base64-32bytes
KUBENOVA_KMS_MASTER_KEY=replace-with-base64-32bytes
KUBENOVA_HTTP_ADDR=:8080
LOG_LEVEL=info
POSTGRES_USER=kubenova
POSTGRES_PASSWORD=kubenova
POSTGRES_DB=kubenova
AGENT_IMAGE=ghcr.io/vaheed/kubenova/agent:dev
MANAGER_URL_PUBLIC=http://kubenova-manager.kubenova-system.svc.cluster.local:8080
# Note: Manager image in GHCR: ghcr.io/vaheed/kubenova/manager:dev
